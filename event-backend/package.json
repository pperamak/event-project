{
  "name": "event-backend",
  "version": "1.0.0",
  "description": "backend for event-project",
  "license": "ISC",
  "author": "Panu Perämäki",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "tsc": "tsc",
    "dev": "NODE_ENV=development node --loader ts-node/esm src/index.ts",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "NODE_ENV=test node --loader ts-node/esm ./node_modules/vitest/vitest.mjs run --no-file-parallelism --globals --environment node",
    "start": "node build/index.js",
    "migration:up": "NODE_ENV=development node --loader ts-node/esm src/util/upMigrate.ts",
    "migration:down": "NODE_ENV=development node --loader ts-node/esm src/util/rollback.ts",
    "migration:up:test": "NODE_ENV=test node --loader ts-node/esm src/util/upMigrate.ts",
    "migration:down:test": "NODE_ENV=test node --loader ts-node/esm src/util/rollback.ts"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^9.0.20",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "tslib": "^2.8.1",
    "umzug": "^3.8.2",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.3.0",
    "supertest": "^7.1.4",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  }
}
